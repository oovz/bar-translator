{
    "extends": "./.wxt/tsconfig.json",
    "compilerOptions": {
        "lib": [
            "DOM",
            "DOM.Iterable",
            "ESNext"
        ],
        "strictNullChecks": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noFallthroughCasesInSwitch": true,
        "isolatedModules": true,
        "jsx": "react-jsx",
        "jsxImportSource": "preact"
    }
}